                        <resource id="Lof_TableRateShipping::shipping_view" title="View Shipping" translate="title" sortOrder="12"/>
                        <resource id="Lof_TableRateShipping::shipping_save" title="Save Shipping" translate="title" sortOrder="15"/>
                        <resource id="Lof_TableRateShipping::shipping_delete" title="Delete Shipping" translate="title" sortOrder="20"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
