<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Lof_TableRateShipping::shipping_elements" title="Lof Table Rate Shipping" translate="title" sortOrder="100">
                    <resource id="Lof_TableRateShipping::shipping" title="Manage Rates" translate="title" sortOrder="10">
